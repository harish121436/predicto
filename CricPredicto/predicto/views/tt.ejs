                                        <div class="card-header text-center">{{player.player}}</div>
                                        <div class="card-body">
                                            <!-- <h4 class="card-title"></h4> -->
                                            <table>
                                                <tr>
                                                    <td>runs scored</td>
                                                    <td>{{player.runs_scored}}</td>
                                                </tr>
                                                <tr>
                                                    <td>balls_faced</td>
                                                    <td>{{player.balls_faced}}</td>
                                                </tr>
                                                <tr>
                                                    <td>dots</td>
                                                    <td>{{player.dots}}</td>
                                                </tr>
                                                <tr>
                                                    <td>got_out</td>
