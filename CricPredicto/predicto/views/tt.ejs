<% layout('layouts/main')  -%>
    <div class="row" ng-controller="ttCtrl">
        <div class="col-lg-10">
            <form class="form-inline" ng-submit="submit()">

                <!-- <input list="batsmen" ng-model="p1">
                                        <input list="batsmen" ng-model="p1">
                                        <input list="batsmen" ng-model="p1"> -->


                <!-- <label class="sr-only" for="p1">Batsman</label> -->
                <input type="text" list="team1" class="form-control mb-2 mr-sm-2 mb-sm-0" ng-model="p1" id="p1" placeholder="Team1" autocomplete="off">

                <!-- <label class="sr-only" for="p2">Bowler</label> -->
                <input type="text" list="team2" class="form-control  mb-2 mr-sm-2 mb-sm-0" ng-model="p2" id="p2" placeholder="Team2" autocomplete="off">

                <!-- <label class="sr-only" for="stadium">Stadium</label> -->
                <input type="text" list="stadiums" class="form-control  mb-2 mr-sm-2 mb-sm-0" ng-model="stadium" id="stadium" placeholder="Stadium"
                    autocomplete="off">


                <datalist id="team1">
                    <option ng-repeat="team in teams" value="{{team}}">
                </datalist>
                <datalist id="team2">
                    <option ng-repeat="team in teams" value="{{team}}">
                </datalist>
                <datalist id="stadiums">
                    <option ng-repeat="stadium in stadiums" value="{{stadium.name}}">
                </datalist>
                <button type="submit" class="btn btn-primary">Submit
                    <span ng-if="sent">
                        <i class="fa fa-spinner" aria-hidden="true"></i>
                    </span>
                </button>
            </form>



            <div class="row" style="margin-top:3rem;">
                <div ng-if="result" class="col-lg-12">
                    <div class="row">
                        <div class="col-md-5">
